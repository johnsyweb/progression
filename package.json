{
  "name": "progress",
  "version": "1.0.0",
  "description": "Progress bar microsite for www.johnsy.com",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css}\"",
    "typecheck": "tsc --noEmit",
    "precommit": "pnpm run format && pnpm run lint && pnpm run typecheck && pnpm run test:run",
    "prepare": "husky",
    "generate:screenshot:build": "tsc scripts/generate-readme-screenshot.ts --outDir scripts --esModuleInterop --skipLibCheck --module es2020 --target es2020 --moduleResolution node",
    "generate:screenshot": "pnpm run generate:screenshot:build && node scripts/generate-readme-screenshot.js",
    "test:sw:build": "tsc scripts/test-service-worker.ts --outDir scripts --esModuleInterop --skipLibCheck --module es2020 --target es2020 --moduleResolution node",
    "test:sw": "pnpm run test:sw:build && node scripts/test-service-worker.js"
  },
  "keywords": [],
  "author": "Pete Johns",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.57.1",
    "html2canvas": "^1.4.1",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "prettier": "^3.8.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^1.6.1"
  },
  "packageManager": "pnpm@10.26.0"
}
