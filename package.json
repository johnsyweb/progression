    "precommit": "pnpm run format && pnpm run lint && pnpm run typecheck && pnpm test"
  },
  "keywords": [],
  "author": "Pete Johns",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^7.2.2",
    "vitest": "^1.0.0"
  }
}
