{
  "name": "progress",
  "version": "1.0.0",
  "description": "Progress bar microsite for johnsy.com",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,html,css}\"",
